{
    "name": "rabsabius/propel",
    "type": "library",
    "description": "Propel2 with index hint support. Extended from propel/propel 2.0.0-alpha11",
    "keywords": [
        "Index Hint",
        "ORM",
        "persistence",
        "Active Record"
    ],
    "homepage": "https://github.com/rabsabius/propel",
    "license": "MIT",
    "authors": [
        {
            "name": "Nico Rabius",
            "email": "nico.rabius@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/yaml": "~2.3||~3.0||~4.0",
        "symfony/console": "~2.3||~3.0||~4.0",
        "symfony/finder": "~2.3||~3.0||~4.0",
        "symfony/validator": "~2.3||~3.0.0||~3.1.0||^3.2.4||~4.0",
        "symfony/filesystem": "~2.3||~3.0||~4.0",
        "symfony/config": "~2.3||~3.0||~4.0",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "monolog/monolog": "~1.3",
        "phpunit/phpunit": "~4.0||~5.0"
    },
    "suggest": {
        "monolog/monolog": "The recommended logging library to use with Propel."
    },
    "autoload": {
        "psr-0": {
            "Propel": "src/"
        }
    },
    "bin": [
        "bin/propel"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
